<h1>Articles Listing Page</h1>
<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Description</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        <% @articles.each do |i| %>
            <tr>
                <td><%= i.title%></td>
                <td><%= i.description %></td>
                <td>placeholder</td>
            </tr>
        <% end %>
    </tbody>
</table>